{
  "name": "reve-sdk",
  "version": "0.3.0",
  "description": "Unofficial SDK for Reve AI image generation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "clean": "rimraf dist",
    "example": "ts-node examples/basic-usage.ts"
  },
  "keywords": [
    "reve",
    "ai",
    "image-generation",
    "sdk",
    "axios"
  ],
  "author": "FlintSH <https://fl1nt.dev>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flintsh/reve-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/flintsh/reve-sdk/issues"
  },
  "homepage": "https://github.com/flintsh/reve-sdk#readme",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "axios-retry": "^4.5.0",
    "form-data": "^4.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
